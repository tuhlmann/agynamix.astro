{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "~/*": [
        "src/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": false,
    "esModuleInterop": true,
    "module": "ESNext",
    "target": "ESNext",
    "strict": true,
    "noImplicitAny": false
   }
}